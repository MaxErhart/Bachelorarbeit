@article{Phipps2016,
abstract = {We present the design and noise performance of a fully cryogenic (T=4 K) HEMT-based charge amplifier for readout of massive semiconductor dark matter detectors operating at sub-Kelvin temperatures. The amplifier has been developed to allow direct detection experiments such as CDMS and EDELWEISS to probe WIMP masses below 10 GeV/{\$}c{\^{}}2{\$} while retaining electromagnetic background discrimination. The amplifier dissipates only 1 mW of power and has a measured noise performance three times better than traditional JFET-based charge amplifiers. The predicted ionization energy resolution using the measured intrinsic amplifier noises and typical detector characteristics is {\$}\backslashsigma{\_}E \backslashapprox 100 \backslash,\backslashtext{\{}eV{\}}{\_}{\{}ee{\}}{\$} (33 electrons). We have measured a calibrated baseline energy resolution of {\$}\backslashsigma{\_}E = 91\backslash,\backslashtext{\{}eV{\}}{\_}{\{}ee{\}}{\$} when coupled to a live CDMS-II detector. To our knowledge, this is the best resolution achieved on such massive ({\$}\backslashapprox{\$}150 pF capacitance) radiation detectors.},
archivePrefix = {arXiv},
arxivId = {1611.09712},
author = {Phipps, A. and Juillard, A. and Sadoulet, B. and Serfass, B. and Jin, Y.},
eprint = {1611.09712},
file = {:E$\backslash$:/Studium/bachelor/lit/HEMTBasedChargeAmp.pdf:pdf},
journal = {arXiv:1611.09712v1},
keywords = {analogue electronic circuits,dark matter detectors,end electronics for detector,front-,hemt amplifiers,readout},
month = {nov},
pages = {1--6},
title = {{A HEMT-Based Cryogenic Charge Amplifier with sub-100 eVee Ionization Resolution for Massive Semiconductor Dark Matter Detectors}},
url = {http://arxiv.org/abs/1611.09712},
year = {2016}
}
