@article{Fleischmann2009,
abstract = {An increasing number of experiments employ low-temperature radiation/particle detectors which are based on a calorimetric detection scheme and operate at temperatures below 100 mK. Metallic magnetic calorimeters use a metallic paramagnetic temperature sensor in tight thermal contact with the X-ray absorber. The magnetization of the sensor is used to monitor the temperature change of the detector upon the absorption of single photons, which is proportional to the absorbed energy. Low-noise high-bandwidth de superconducting quantum interference devices read out the small changes in magnetization. An energy resolution of Delta E(FWHM) = 2.7 eV was obtained for X-ray energies up to 6 keV.},
