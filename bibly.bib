@article{Agnese2018,
abstract = {We report the result of a blinded search for Weakly Interacting Massive Particles (WIMPs) using the majority of the SuperCDMS Soudan dataset. With an exposure of 1690 kg days, a single candidate event is observed, consistent with expected backgrounds. This analysis (combined with previous Ge results) sets an upper limit on the spin-independent WIMP--nucleon cross section of {\$}1.4 \backslashtimes 10{\^{}}{\{}-44{\}}{\$} ({\$}1.0 \backslashtimes 10{\^{}}{\{}-44{\}}{\$}) cm{\$}{\^{}}2{\$} at 46 GeV/{\$}c{\^{}}2{\$}. These results set the strongest limits for WIMP--germanium-nucleus interactions for masses {\$}{\textgreater}{\$}12 GeV/{\$}c{\^{}}2{\$}.},
